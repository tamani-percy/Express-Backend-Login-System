                      </figure>
                    </td>
                    <td class="user-temp">
                      <!-- <div class="ldBar" data-value="50"> </div> -->
  
                      <div data-stroke-width="40" id="myItem2" data-preset="energy" data-precision="0.01" data-max="40" data-min="35"> </div>
                      <script >
                        /* construct manually */
                        var bar1 = new ldBar("#myItem2");
                        /* ldBar stored in the element */
                        var bar2 = document.getElementById("myItem2").ldBar;
                        bar1.set(36.8);
                      </script>
                      </script>
                    </td>
                    <td class="user-time">
                      <h4>
                        09:42:54 <br />
                        12/07/2022
                    </h4>
                    </td>
                  </tr>
                  <tr>
                    <td class="user-face">
